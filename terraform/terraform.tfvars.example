# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars should be gitignored

aws_region  = "eu-central-1"
environment = "prod"

# Required: Your domain for HTTPS
domain_name = "openclaw.example.com"

# Optional: Route53 hosted zone ID for automatic DNS
# Leave empty to manually add DNS records
route53_zone_id = ""

# EC2 Configuration
instance_type   = "t3.small"
ebs_volume_size = 20

# Budget option: set to false to disable NAT Gateway (~$32/month savings)
# Less secure but significantly cheaper
enable_nat_gateway = true
